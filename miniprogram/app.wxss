/**app.wxss**/

page {
  background-color: #f5f5f5;
}

.navigator-hover { 
  background-color: rgba(128,128,128,.1);
  opacity: .7;
}



.container {
  padding: 60rpx;
  box-sizing: border-box;
}

.main {
  color: rgb(3, 169, 243);
}

text {
  font-weight: 300;
}

.text_between {
  text-align: justify;
  display: inline-block;
  vertical-align: middle;  
}
 

.tip_container {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tip_txt {
  font-size: 32rpx;
  line-height: 1.2;
  color: #666;
}

.loading {
  margin: 40rpx 0;
}
.loading_txt {
  font-size: 24rpx;
  color: #ccc;
}




.nothing {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.no_pic {
  width: 150rpx;
  height: 150rpx;
  margin-bottom: 40rpx;
}
.no_txt {
  font-size: 24rpx;
  line-height: 1.6;
  color: rgb(3, 169, 243);
}

.no_txt_bd {
  border-bottom: 1rpx solid rgb(3, 169, 243);
}





.like {
  width: 88rpx;
  height: 88rpx; 
  background: url(data:image/png;base64,) center / 48rpx no-repeat;
}



.liked {
  background: url(data:image/png;base64,) center / 48rpx no-repeat;
}


  
.share {
  width: 88rpx;
  height: 88rpx;  
  background: url(data:image/png;base64,) center / 48rpx no-repeat;
}

.share_btn {
  width: 88rpx;
  height: 88rpx;
  padding: 0;
  display: block; 
  background-color: transparent;
}

.mp3 {
  height: 88rpx; 
  width: 88rpx;
  background: url(data:image/png;base64,) center / 48rpx no-repeat;
}

.mp3ed {
  height: 88rpx; 
  width: 88rpx;
  background: url(data:image/png;base64,) center / 48rpx no-repeat;
}
 

.download {
  height: 88rpx; 
  width: 88rpx;
  background: url(data:image/png;base64,) center / 42rpx no-repeat;
}